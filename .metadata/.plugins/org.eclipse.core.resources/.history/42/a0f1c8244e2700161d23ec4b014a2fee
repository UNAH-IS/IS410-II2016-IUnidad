package clases;

public class Alumno extends Persona{
	private String cuenta;

	public Alumno(
			String nombre,
			String apellido,
			String genero,
			int edad,
			Carrera carrera,
			String horario,
			String cuenta){
		super(nombre, apellido, genero, edad, carrera, horario);
		this.cuenta = cuenta;
	}

	public String toString(){
		return super.toString() + cuenta;
	}

}
