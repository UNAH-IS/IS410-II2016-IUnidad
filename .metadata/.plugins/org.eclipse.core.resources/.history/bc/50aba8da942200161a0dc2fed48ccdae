package clases;

public class Arreglos {
	//Variables globales para la clase
	private int x;
	public Arreglos(){
		//Logica del programa
		x = 10;
		System.out.println(sumar(4,5));
	}
	
	public static void main(String[] args) {
		Arreglos a = new Arreglos();
	}
	
	public int sumar(int a, int b){
		return a+b;
	}
}
