package clases;

public class Arreglos {
	//Variables globales para la clase
	private int[] enteros;
	public Arreglos(){
		//Logica del programa
		enteros = new int[5];
		enteros[0]= 10;
		enteros[1]= 20;
		enteros[2]= 30;
		enteros[3]= 40;
		enteros[4]= 50;
		enteros[5]= 60;
		

	}

	public static void main(String[] args) {
		new Arreglos(); //Instancia anonima
	}
}
