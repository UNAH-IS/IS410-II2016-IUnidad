package implementacion;
//Shift+Ctrl+O
import java.util.ArrayList;

import clases.Producto;

public class Principal {
	//Definicion de atributos
	private ArrayList<Producto> productos;
	//<> RawType, en el caso de ArrayList
	//indica el tipo de dato que va a almacenar

	public Principal(){
		productos = new ArrayList<Producto>();
		productos.add(new Producto("Computadora"));
		productos.add(new Producto("Desodorante"));
		productos.add(new Producto("Jabon Extra"));
		productos.add(new Producto("Licuadora"));
		productos.add(new Producto("Gelatina moco de gorila"));
		System.out.println("Cantidad elementos: " + productos.size());
	}
	public static void main(String[] args) {
		new Principal();
	}
}
